{
    "openapi": "3.0.0",
    "info": {
        "title": "HNG Portal API Documentation",
        "description": "API documentation for HNG Portal backend",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "/",
            "description": "Base API Server"
        }
    ],


    "paths": {
        "/api/faq": {
            "get": {
                "tags": [
                    "FAQ"
                ],
                "summary": "Get list of FAQs",
                "operationId": "3c5e434ed5df3403c3229ae9b6a177b1",
                "parameters": [
                    {
                        "name": "type",
                        "in": "query",
                        "description": "FAQ type: company or talent",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "enum": [
                                "company",
                                "talent"
                            ]
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "FAQs retrieved successfully"
                    }
                }
            }
        },
        "/api/faq/{faq}": {
            "get": {
                "tags": [
                    "FAQ"
                ],
                "summary": "Get a single FAQ by ID",
                "operationId": "7c9e8435e2a5a34a0c625c0eb92064e4",
                "parameters": [
                    {
                        "name": "faq",
                        "in": "path",
                        "description": "FAQ ID",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "uuid"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "FAQ retrieved successfully"
                    },
                    "404": {
                        "description": "FAQ not found"
                    }
                }
            }
        },
        "/api/jobs": {
            "get": {
                "tags": [
                    "Jobs"
                ],
                "summary": "List all jobs with filters",
                "operationId": "9538d30d530d4c3680ea31fe228944e2",
                "parameters": [
                    {
                        "name": "q",
                        "in": "query",
                        "description": "Search by job title or description",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "location_id",
                        "in": "query",
                        "description": "Filter by location ID",
                        "required": false,
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "location",
                        "in": "query",
                        "description": "Filter by location name",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "job_type_id",
                        "in": "query",
                        "description": "Filter by job type ID",
                        "required": false,
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "track_id",
                        "in": "query",
                        "description": "Filter by track ID",
                        "required": false,
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "category_id",
                        "in": "query",
                        "description": "Filter by category ID",
                        "required": false,
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "min_price",
                        "in": "query",
                        "description": "Minimum price filter",
                        "required": false,
                        "schema": {
                            "type": "number"
                        }
                    },
                    {
                        "name": "max_price",
                        "in": "query",
                        "description": "Maximum price filter",
                        "required": false,
                        "schema": {
                            "type": "number"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successfully retrieved list of jobs"
                    }
                }
            }
        },
        "/api/jobs/{job}": {
            "get": {
                "tags": [
                    "Jobs"
                ],
                "summary": "Get a single job by ID",
                "operationId": "9c31b377241c635bf2819779f358d414",
                "parameters": [
                    {
                        "name": "job",
                        "in": "path",
                        "description": "Job ID",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Job details retrieved"
                    },
                    "404": {
                        "description": "Job not found"
                    }
                }
            }
        },
        "/api/jobs/{job}/related": {
            "get": {
                "tags": [
                    "Jobs"
                ],
                "summary": "Get related jobs",
                "operationId": "5a5c9e3acd17081f3093c654e4766ca1",
                "parameters": [
                    {
                        "name": "job",
                        "in": "path",
                        "description": "Job ID to fetch related jobs for",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Related jobs retrieved successfully"
                    },
                    "404": {
                        "description": "Job not found"
                    }
                }
            }
        },
        "/api/jobs/search": {
            "get": {
                "tags": [
                    "Jobs"
                ],
                "summary": "Search jobs using keyword",
                "operationId": "0c0290a245c580348856b16cf6be0b81",
                "parameters": [
                    {
                        "name": "q",
                        "in": "query",
                        "description": "Search keyword",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Search results returned"
                    }
                }
            }
        }
    },
    "tags": [
        {
            "name": "FAQ",
            "description": "FAQ"
        },
        {
            "name": "Jobs",
            "description": "Jobs"
        }
    ]
}
